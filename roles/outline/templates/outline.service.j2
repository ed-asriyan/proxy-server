[Unit]
Description=outline
After=network.service

[Service]
User={{ user }}
AmbientCapabilities=CAP_NET_BIND_SERVICE
ExecStart=/home/{{ user }}/{{ executable_name }} -replay_history={{ replay_history }} -config /home/{{ user }}/config.yml -metrics 127.0.0.1:{{ metrics_port }}
Restart=always

[Install]
WantedBy=multi-user.target
