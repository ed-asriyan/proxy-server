global:
  scrape_interval: 1m
scrape_configs:
  - job_name: outline-server-ss
    static_configs:
      - targets:
          - '127.0.0.1:{{ metrics_port }}'
        labels:
          instance: '{{ server_name }}'
  - job_name: node-exporter
    static_configs:
      - targets:
          - '127.0.0.1:{{ node_exporter_port }}'
        labels:
          instance: '{{ server_name }}'
